#!/home/mikey/Desktop/PythonProj/hotel_click/hotelenv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'hotel-click','console_scripts','hotel'
__requires__ = 'hotel-click'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('hotel-click', 'console_scripts', 'hotel')()
    )
